{
  "manifest_version": 2,
  "name": "MyCloudCSSChange",
  "description": "This extension fixes the hidden buttons for shutdown.",
  "version": "4.0",
  "icons":{
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon48.png"
  },
  "permissions": [
    "activeTab"
  ],
  "background":{
    "scripts":["bg.js"]
  }
  
}